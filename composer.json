{
    "name": "beyondcode/laravel-view-xray",
    "description": "Take a look into your Laravel views.",
    "keywords": [
        "beyondcode",
        "laravel-view-xray"
    ],
    "homepage": "https://github.com/beyondcode/laravel-view-xray",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "marcel@beyondco.de",
            "homepage": "https://beyondcode.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.6.*|5.7.*|5.8.*|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.6|^3.7|^3.8|^4.0|^5.0",
        "phpunit/phpunit": "^7.0|^8.0",
        "spatie/phpunit-snapshot-assertions": "^1.3|^2.0"
    },
    "autoload": {
        "psr-4": {
            "BeyondCode\\ViewXray\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeyondCode\\ViewXray\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "BeyondCode\\ViewXray\\ViewXrayServiceProvider"
            ]
        }
    }
}
